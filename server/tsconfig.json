{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2016",
        "strict": true,
        "noImplicitAny": true,
        "removeComments": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "rootDir": "./src",
        "outDir": "./dist",
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}